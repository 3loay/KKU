          function checkConfirmNum(){
                var num1 = document.getElementById("myForm:num1").value;
                var numconf1 = document.getElementById("myForm:numconf1").value;
                
                var num3 = document.getElementById("myForm:num3").value;
                var numconf3 = document.getElementById("myForm:numconf3").value;
                
                
                  if(num1 != "" && numconf1 != "" && num1 != numconf1) {
                    document.getElementById("myForm:numconf1").style.backgroundColor='rgb(255,198,132)';
                    document.getElementById("myForm:numconf1").value = ''; 
                  }
                
                
                  if(num3 != "" && numconf3 != "" && num3 != numconf3) {
                     document.getElementById("myForm:numconf3").style.backgroundColor='rgb(255,198,132)';
                     document.getElementById("myForm:numconf3").value = ''; 
                  }
             }
    
    
    
        
         function validateMandatoryLanguages(){
            var valid = true;
            var nonSelectedLangs = 0;
            var langId = document.getElementsByName("myForm:langType");
            if(langId.length == 0){
                return;
            }
            
            var x;
            for(x=0; x < langId.length; x++){
                if(langId[x].checked){
                   nonSelectedLangs++; 
                }
            }
            
            if(nonSelectedLangs == 0){
                alert(errorLang);
                validation = false;
                langId[0].focus();
            }
        }
        
        
        
        function validateMandatoryIdentifitionLanguages(){
            var valid = true;
            var nonSelectedDocs = 0;
            var nonSelectedLangs = 0;
            var identDocs = document.getElementsByName("myForm:identificationId");
            var langId = document.getElementsByName("myForm:langType");
            
            if(identDocs.length == 0){
                return;
            }
            
            var i;
            for(i=0; i < identDocs.length; i++){
                if(identDocs[i].checked){
                   nonSelectedDocs++; 
                }
            }
            
            if(nonSelectedDocs == 0){
                alert(errorNoIdentifications);
                validation = false;
                valid = false;
                identDocs[0].focus();
            }
        

        if (valid){
            if(langId.length == 0){
                return;
            }
            
            var x;
            for(x=0; x < langId.length; x++){
                if(langId[x].checked){
                   nonSelectedLangs++; 
                }
            }
            
            if(nonSelectedLangs == 0){
                alert(errorLang);
                validation = false;
                langId[0].focus();
            }
          }
            
            
            
        }
         
        function validateBridgeCertApplication(){
           var isBridge  =  document.getElementById('myForm:isBrdge').value;
           if(isBridge == 'true' || isBridge == true){
               validateHighStudyStudentCertApplication();
           }
         }
        
         
        function submitHighStudyApplicationOnline(commandLink){
         
            validation = true;
            if(commandLink != 'commandUpdateLink'){
                validateHighStudyDegreeCode();
            }
            validateNewApplication();
            validateBasicStudentApplication();
            validateGenderApplication();
            validateBrithDateApplication();
            validateJobApplication();
            validateHighStudyStudentCertApplication();
            validateStudyingType();
            validateContactApplication();
            validateMandatoryIdentifitionLanguages();
            validateHighStudyApplication(commandLink);
        }
        
        function IsNumeric(sText){
            var ValidChars = "0123456789.";
            var IsNumber=true;
            var Char;
        
             if(sText.length <= 0)
                 return false;
        
             for (i = 0; i < sText.length && IsNumber == true; i++){ 
                 Char = sText.charAt(i); 
                 if (ValidChars.indexOf(Char) == -1){
                    IsNumber = false;
                    break;
                 }
              }
             return IsNumber;
        }
        
        function IsNumericOnly(sText){
             var ValidChars = "0123456789";
             var IsNumber=true;
             var Char;
        
             if(sText.length <= 0)
                 return false;
        
             for (i = 0; i < sText.length && IsNumber == true; i++){ 
                 Char = sText.charAt(i); 
                 if (ValidChars.indexOf(Char) == -1){
                    IsNumber = false;
                    break;
                 }
              }
             return IsNumber;
        }
        
        function IsNumericTelphone(sText){
             var ValidChars = "0123456789/.-\\";
             var IsNumber=true;
             var Char;
        
             if(sText.length <= 0)
                 return false;
        
             for (i = 0; i < sText.length && IsNumber == true; i++){ 
                 Char = sText.charAt(i); 
                 if (ValidChars.indexOf(Char) == -1){
                    IsNumber = false;
                    break;
                 }
              }
             return IsNumber;
        }     
        
	function onKeySubmitForm(e,textId){
             var key = window.event ? e.keyCode : e.which;
             var keychar = String.fromCharCode(key);
		           
	     if(key == 13)
                document.getElementById("myForm:appLink").onclick();
        }

	function onKeyPressBlockCharsAndPunctuationMarks(e,textId){
             var key = window.event ? e.keyCode : e.which;
             var keychar = String.fromCharCode(key);
            
             if(key == 13){
                if(textId.value == '' || IsNumeric(textId.value)){ 
                    if(document.getElementById("myForm:appLink")!= null)
                   document.getElementById("myForm:appLink").onclick();
                }
                else{
                   textId.disabled=true;
                   alert(errorChars);
                   textId.disabled=!textId.disabled;
                   //textId.focus();
                }
             }
             
             if(IsNumeric(keychar))
                 return true;
             return false;
        }
        
        function onBlurblockCharsAndPunctuationMarks2(textId){
             var text = textId.value; 

             if(text != ''){
	     	if(!IsNumeric(text)){
                   alert(errorChars);
		   textId.focus();
		}
            }
        }
        
        
        function onBlurblockCharsAndPunctuationMarks(textId){
	     var text = textId.value; 

	     if(text != ''){
	     	if(!IsNumeric(text)){
                   alert(errorChars);
		   textId.focus();
		}
                else if(textId.value > 100){
                   alert(errorMaxNumber);
                   textId.focus();
                }
	     }
        }
        
        function onBlurblockCharsAndPunctuationTofelMarks(textId, maxMark, msgs){
	     var text = textId.value; 

	     if(text != ''){
