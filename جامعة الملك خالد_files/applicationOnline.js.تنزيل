                   textId.focus();
                }
	     }
        }
        
        function onKeyPressBlockNumbers(e,textId){
             var key = window.event ? e.keyCode : e.which;
             var keychar = String.fromCharCode(key);
            
             if(key == 13){
                if(textId.value  == '' || !IsNumeric(textId.value)){ 
                   document.getElementById("myForm:appLink").onclick();
                }
                else{
                    textId.disabled=true;
                    alert(errorNumbers);
                    textId.disabled=!textId.disabled;
                    textId.focus();
                }
             }
            
             if(IsNumeric(keychar))
                return false;
             return true;
        }

        function onKeyBlockPunctuationsMark(e,textId){
             var key = window.event ? e.keyCode : e.which;
             var keychar = String.fromCharCode(key);
           
             if(key == 13){
                 var text = textId.value; 
                 var notCharacter = "<>?!@#$%^&*(){}*+='[]/\\" +'"';
                 var found = false; 
                 for(i = 0 ;i < notCharacter.length; i++){
                     if(text.indexOf(notCharacter.charAt(i)) != -1){
                        textId.disabled=!textId.disabled;
                        alert(errorSpecialChars);
                        textId.disabled=!textId.disabled;
                        found = true; 
                        textId.focus(); break;
                     }
                 }
                 if(!found){
                     document.getElementById("myForm:appLink").onclick();
                 }
             }
               
             var notCharacter = "<>?!@#$%^&*(){}*+='[]/\\" + '"'; 
             for(i = 0 ;i < notCharacter.length; i++){
                 if(keychar.indexOf(notCharacter.charAt(i)) != -1){
                    return false;
                 } 
             }
             return true;
        }   
        
