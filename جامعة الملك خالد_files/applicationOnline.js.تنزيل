	function maxChoices(){
             
             var treeName = "myForm:serverTreeApp";// + degree;
            
             var x,i,j;
             var count =0; var z = 0;
             var v1 = -1, v2 = -1, v3 = -1;
             var arrayVar = new Array();
             var countVar  = 0;
             var foundVar = false;
            
            
             for(i=0;i<20;i++){
                for(j=0;j<50;j++){
                   try{
                        z = z + 1;
                        x = document.getElementById(treeName+":0:"+i+":"+j+":Check").checked;
                        
                        if(x == true){
                           count= count + 1;
                           countVar = countVar + 1;
                           arrayVar[countVar - 1] = z; 
                        }
                   }
                   catch(e){break;}
                }
              }
            
              z=0;
            
             if(count >= max){
                  for(i=0;i<20;i++){
                    for(j=0;j<50;j++){
                       try{
                            z = z + 1;
                            foundVar = false;
                            for(l = 0;l<countVar;l++){
                                if(z == arrayVar[l]){
                                   foundVar = true;
                                }
                            }
                            if(!foundVar){
                               document.getElementById(treeName+":0:"+i+":"+j+":Check").disabled=true;
                            }
                       }
                      catch(e){break;}
                    }
                  }              
             }      
             
             if(count< max){
                for(i=0;i<20;i++){
                  for(j=0;j<50;j++){
                     try{
                        document.getElementById(treeName+":0:"+i+":"+j+":Check").disabled=false;  
                     }
                     catch(e){break;}
                  }
                }              
             }                
        }
        
        
	function setPriorityOfValue(element , value, size, texts, index){
          
          var valueIndex; var j; var n; var sel = 0;
          
          for(j = 1 ; j <= size ; j++) {      
          
            if(j != value){
               
               for(n = 0 ; n < size ; n++){
                   valueIndex = document.getElementById("myForm:Tabel:"+n+":index").value;
                   if(j == valueIndex){
                      n = size;
                      sel = 1;
                   }
               }
               
               if(sel == 0){
                       var elOptNew = document.createElement('option');
                       if(j == 0){
                          elOptNew.text = texts;
                       }
                       else{
                          elOptNew.text = j;
                       }
                       elOptNew.value = j;	
                       try {
                          element.add(elOptNew, null); 
                       }catch(ex) {
                          element.add(elOptNew);
                       }
               }
               sel = 0;
            }
          }
          element.value = index;
       }
       
       function trim(s)
{
	return rtrim(ltrim(s));
}


    function checkSocialData(){
            var pass = true;


             if(document.getElementById("myForm:socialType").selectedIndex == 0) {
               document.getElementById("myForm:socialType").style.backgroundColor='rgb(255,198,132)';
               if(pass)
                  document.getElementById("myForm:socialType").focus(); 
               pass = false;
            }
            else{
                document.getElementById("myForm:socialType").style.backgroundColor='white';
            }
            
            
            
             if(document.getElementById("myForm:socialAcount").value =='') {
             
               document.getElementById("myForm:socialAcount").style.backgroundColor='rgb(255,198,132)';
               if(pass)
                  document.getElementById("myForm:socialAcount").focus(); 
               pass = false;
            }
            else{
              
                document.getElementById("myForm:socialAcount").style.backgroundColor='white';
            }
            

            if(pass){
                var cmd = document.getElementById('myForm:saveSocialRecord');
                cmd.onclick();
            }
            
        }

        function blockEnglishLang(textId){
            var text = textId.value; 
            var notCharacter = "1234567890.<>?!@#$%^&*(){}*-+_='[]/\\" + '"'; 
            var found = false; 
            for(i = 0 ;i < notCharacter.length; i++){
                 if(text.indexOf(notCharacter.charAt(i)) != -1){
                    textId.disabled=true; 
                    alert(errorNumbers);
                    found = true; 
                    textId.disabled=!textId.disabled;
                    textId.focus(); break;
                 }
            }

            var english = /^[A-Za-z]*$/;
            for (i=0;i<text.length;i++) {
                if(english.test(text[i])) {
                    return true;
                }
            }
            return false;
        }        

        function blockArabicLang(textId){
            var text = textId.value; 
            var notCharacter = "1234567890.<>?!@#$%^&*(){}*-+_='[]/\\" + '"'; 
            var found = false; 
            for(i = 0 ;i < notCharacter.length; i++){
                 if(text.indexOf(notCharacter.charAt(i)) != -1){
                    textId.disabled=true; 
                    alert(errorNumbers);
                    found = true; 
                    textId.disabled=!textId.disabled;
                    textId.focus(); break;
                 }
            }

            var arabic = /[\u0600-\u06FF]/;
            for (i=0;i<text.length;i++) {
                if(arabic.test(text[i])) {
                    return true;
                }
            }
            return false;
        }    
        function onKeyBlockEnglishLang(e,textId,message){
             var key = window.event ? e.keyCode : e.which;
             var keychar = String.fromCharCode(key);

             if(key !=13 && (key >  66 && key < 123)){
				alert(message);
                return false;
             }
             
             if(key == 13){
                 var text = textId.value; 
                 var notCharacter = "1234567890.<>?!@#$%^&*(){}*-+_='[]/\\" + '"'; 
                 var found = false; 
                 for(i = 0 ;i < notCharacter.length; i++){
                     if(text.indexOf(notCharacter.charAt(i)) != -1){
                        textId.disabled=true; 
                        alert(errorNumbers);
                        found = true; 
                        textId.disabled=!textId.disabled;
                        textId.focus(); break;
                     }
                 }
                 if(!found){
                     document.getElementById("myForm:appLink").onclick();
                 }
             }
               
             var notCharacter = "1234567890.<>?!@#$%^&*(){}*-+_='[]/\\" + '"'; 
             for(i = 0 ;i < notCharacter.length; i++){
                 if(keychar.indexOf(notCharacter.charAt(i)) != -1){
                    return false;
                 } 
             }
             return true;
        }
     
        
        function onKeyBlockArabicLang(e,textId,message){
             var key = window.event ? e.keyCode : e.which;
             var keychar = String.fromCharCode(key);
             
             var notCharacter = "1234567890.<>?!@#$%^&*(){}*-+_='[]/\\" + '"'; 
             for(i = 0 ;i < notCharacter.length; i++){
                 if(keychar.indexOf(notCharacter.charAt(i)) != -1){
                    return false;
                 } 
             }
             
             if(key !=13 && (key < 65 || key > 122)){
                 if(key !=32) // by mustafa
		   alert(message);
                return false;
			
             }
             
             if(key == 13){
                 var text = textId.value; 
                 var notCharacter = "1234567890.<>?!@#$%^&*(){}*-+_='[]/\\" + '"'; 
                 var found = false; 
                 for(i = 0 ;i < notCharacter.length; i++){
