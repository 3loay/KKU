        function onBlurNationalNo(textId,message){
            if(textId.value != '' && !IsNumeric(textId.value)){
                alert(errorChars);
                //textId.focus();
             }
            else{
                if(textId.value != ''){
                    if(textId.value.length != 10){
                        alert(message);
                        //textId.focus();
                    }
                }
            }
        }
        
        function onBlurMaxUnivGpa(textId){
             var text = document.getElementById("myForm:maxTransGpa").value; 
             
             if(text != '' ){
                 if(!IsNumeric(text)){
                    alert(errorChars);
                    textId.focus();
                 }
                 else if(textId.value > 100){
                   alert(errorMaxNumber);
                   textId.focus();
                 }
                 else if(document.getElementById("myForm:transGpa").value != ''){
                    var maxMark = parseInt(text);
                    var mark = parseInt(document.getElementById("myForm:transGpa").value);
                    if(maxMark < mark){
                       alert(errorMaxMark);
                       textId.focus();
                    }
                 }
             }
        }
        
