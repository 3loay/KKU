        function changeDegree(){
        
            var spanHign = document.getElementById("LOW_HIGH_EDUCATION");
            var spanLow  = document.getElementById("LOW_EDUCATION");
           
            var levels = level.split("@");
            var lowLevel  = levels[0].split(",");
            var highLowLevel = "High";
            var isGraduated = "1";

            for(i = 0; i <lowLevel.length; i++){
                if(document.getElementById("myForm:degree").value == lowLevel[i]){
                   highLowLevel = "Low";   isGraduated = "0"; break;
                }   
            }
            
            if( highLowLevel == "Low"){
                spanLow.style.display ='';
                spanHign.style.display ='none';
            }            
            else if( highLowLevel == "High"){
                spanLow.style.display ='none';
                spanHign.style.display ='';
            } 

            document.getElementById("myForm:isGraduated").value = isGraduated;
        }
        
        function checkPassword(){
            var pass1 = false, pass2 = false;
            
            var password = document.getElementById("myForm:password").value;
            var confirmPassword = document.getElementById("myForm:confirmPassword").value;
            
            if(password != '' && !IsNumericOnly(password)){
               pass1 = true;
               alert(errorChars);
               document.getElementById("myForm:password").focus();
            }
            
            if(!pass1){
                if(password.length != "" && password.length < 6){
                    alert(errorPassword);
                    document.getElementById("myForm:password").focus()
                }
                else if(password != "" && confirmPassword != "" && password != confirmPassword) {
                    alert(errorConfirmPassword);
                    document.getElementById("myForm:confirmPassword").focus()
                }
            }
        }
        
        function checkConfirmPassword(){
        
            var pass1 = false, pass2 = false;
            var password = document.getElementById("myForm:password").value;
            var confirmPassword = document.getElementById("myForm:confirmPassword").value;
           
            if(confirmPassword != '' && !IsNumericOnly(confirmPassword)){
               pass2 = true;
               alert(errorChars);
               document.getElementById("myForm:confirmPassword").value='';
               document.getElementById("myForm:confirmPassword").focus();
            }
            
            if(!pass2){
                if(confirmPassword.length != "" && confirmPassword.length < 6){
                    alert(errorPassword);
                     document.getElementById("myForm:confirmPassword").value='';
                    document.getElementById("myForm:confirmPassword").focus()
                }
                else if(password != "" && confirmPassword != "" && password != confirmPassword) {
                    alert(errorConfirmPassword);
                     document.getElementById("myForm:confirmPassword").value='';
                    document.getElementById("myForm:confirmPassword").focus()
                }
            }
        }
        
        function checkEmail(){
            
            var email = document.getElementById("myForm:email").value;
           // var confirmEmail = document.getElementById("myForm:confirmEmail").value;
            var passEmail = false;
            
            if( !emailCheck(email,errorMail,langStr)){
               passEmail = true; 
               document.getElementById("myForm:email").value='';
               setTimeout(function(){document.getElementById("myForm:email").focus()},0);
            }
            
            if(!passEmail ) {
               passEmail = true; 
               //alert(errorConfirmEmail);
               //document.getElementById("myForm:confirmEmail").focus();
            }
        }
        
        function checkConfirmEmail(){
            
            var email = document.getElementById("myForm:email").value;
            var confirmEmail = document.getElementById("myForm:confirmEmail").value;
            var passEmail = false;
            
            if(confirmEmail != '' && !emailCheck(confirmEmail,errorMail,langStr)){
               passEmail = true;
               document.getElementById("myForm:confirmEmail").value='';
               document.getElementById("myForm:confirmEmail").focus();
            }
        
            if(!passEmail && email != "" && confirmEmail != "" && email != confirmEmail) {
                alert(errorConfirmEmail);
                document.getElementById("myForm:confirmEmail").value='';
                document.getElementById("myForm:confirmEmail").focus();
            }
        }
        
        function loginApplicationOnline(malGender){
            validation = true;
            if(document.getElementById("myForm:nationalNo").value == "") {
               document.getElementById("myForm:nationalNo").style.backgroundColor='rgb(255,198,132)';
               validation = false;
               document.getElementById("myForm:nationalNo").focus(); 
            }
            else{
               document.getElementById("myForm:nationalNo").style.backgroundColor='white';
            }
            
            if(malGender == 'true') // 16/8/09 they asked to stop qodorat validation coz entesab do not have qodorat
            {
                /*if(document.getElementById("myForm:capabilities").value == "") {
                   document.getElementById("myForm:capabilities").style.backgroundColor='rgb(255,198,132)';
                   if(validation)
                      document.getElementById("myForm:capabilities").focus(); 
                   validation = false; 
                }
                else{
                    document.getElementById("myForm:capabilities").style.backgroundColor='white';
                }*/
            }
            /*if(document.getElementById("myForm:tahselMark").value == "") {
               document.getElementById("myForm:tahselMark").style.backgroundColor='rgb(255,198,132)';
               if(validation)
                  document.getElementById("myForm:tahselMark").focus(); 
               validation = false; 
            }
            else{
                document.getElementById("myForm:tahselMark").style.backgroundColor='white';
            }*/
            
            
            
            if(validation)
               document.getElementById("myForm:commandLink").onclick();
        }   
        
        
           
        function checkNationalAndDegree(){
            validation = true;
            if(document.getElementById("myForm:nationalNo").value == "") {
               document.getElementById("myForm:nationalNo").style.backgroundColor='rgb(255,198,132)';
               validation = false;
               document.getElementById("myForm:nationalNo").focus(); 
            }
            else{
               document.getElementById("myForm:nationalNo").style.backgroundColor='white';
            }
            
            if(document.getElementById("myForm:degreeCode").value == "-1") {
               document.getElementById("myForm:degreeCode").style.backgroundColor='rgb(255,198,132)';
               validation = false;
               document.getElementById("myForm:degreeCode").focus(); 
            }else{
               document.getElementById("myForm:degreeCode").style.backgroundColor='white';
            }
            
            if(validation)
               document.getElementById("myForm:commandLink").onclick();
        }
        
        
