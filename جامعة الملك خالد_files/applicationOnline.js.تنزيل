	function change(radio){
	
            var span = document.getElementById("LOW");
            //var TransferLabel=document.getElementById("myForm:_id64").checked;
            if(radio.value == '1'){
               span.style.display =''; 
            }
            else{
               span.style.display ='none';
            }
        }
        
        function changeJob(radio){
	
            var span = document.getElementById("JobDiv");
            //var jobLabel=document.getElementById("myForm:_id92").checked;
            
            if(radio.value == '1'){
               span.style.display =''; 
            }
            else{
               span.style.display ='none';
               //document.getElementById("myForm:organization").selectedIndex= 0;
               //document.getElementById("myForm:indefiniteJob").value="";
            }
        }

        function changeHoppy(radio){
            var span = document.getElementById("hoppyDiv");
            if(radio.value == '1'){
                span.style.display ='';
                document.getElementById("myForm:hoppyDivHidden").value = 'block';
            } else{
                span.style.display ='none';
                document.getElementById("myForm:hoppyDivHidden").value = 'none';
            }
        }

        function changeSaheid(radio){
            var span = document.getElementById("chiledSaheidDiv");
            if(radio.value == '1'){
                span.style.display ='';
                document.getElementById("myForm:chiledSaheidHidden").value = 'block';
            } else{
                span.style.display ='none';
                document.getElementById("myForm:chiledSaheidHidden").value = 'none';
            }
        }


        
