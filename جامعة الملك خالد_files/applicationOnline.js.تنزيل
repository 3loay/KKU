        function validateLowStudyApplication(commandLink){
            var validationError = true;
            
            var isBridge  =  document.getElementById('myForm:isBrdge').value;
            if(isBridge == false || isBridge == 'false'){
                if(validation){
                    /*if(document.forms['myForm'].elements['myForm:isTransfer'][0].checked == true) {
                       var mark = parseInt(document.getElementById("myForm:transGpa").value);
                       var maxMark = parseInt(document.getElementById("myForm:maxTransGpa").value);
                       if(mark > maxMark){
                          alert(errorMaxMark);  
                          validationError = false;
                          document.getElementById("myForm:maxTransGpa").focus();
                       }
                    }*/
                    var certYear = document.getElementById("myForm:certYear").value;
                    var DOBYear = document.getElementById("myForm:DOBYear").value;
                    
                    if(parseInt(DOBYear) + 16 > parseInt(certYear)){
                       validationError = false;
                       alert(errorYear2)
                    }
                }
            }
          
            if(validation && validationError){
               document.getElementById("myForm:"+commandLink).onclick();
            }   
        }
        
        function validateHighStudyApplication(commandLink){
            var validationError = true
            
            if(validation){
               var mark = parseInt(document.getElementById("myForm:univGpa").value);
               var maxMark ;
               var isDiplomPar =  document.getElementById('myForm:isDiplom').value;
                if(isDiplomPar == 'true'){
                   maxMark = parseInt(document.getElementById("myForm:maxUnivGpaDiplom").value);
                } else {
                  maxMark = parseInt(document.getElementById("myForm:maxUnivGpa").value);
               }
               
               if(mark > maxMark){
                  alert(errorMaxMark);  
                  validationError = false;
                  document.getElementById("myForm:maxUnivGpa").focus();
               }
            }
            
            if(validation && validationError){
               document.getElementById("myForm:"+commandLink).onclick();
            }   
        }
        
        function submitApplicationOnline(highStudy, commandLink, male){
            isMale = male;
            if(highStudy == 'false')
               submitLowStudyApplicationOnline(commandLink);
            else if(highStudy == 'true')
               submitHighStudyApplicationOnline(commandLink); 
        }
        
        function submitLowStudyApplicationOnline(commandLink){
            validation = true;
            
            validateNewApplication();
           
            validateBasicStudentApplication();
      
            validateBrithDateApplication();
    
            validateJobApplication();

            validateBridgeCertApplication();

            validateSecondaryStudentCertApplication();

            validateStudyingType();

            validateCertificationType();

            validateContactApplication();

            //validatePostBranchesApplication();

            validateMandatoryIdentifitionLanguages();

            validateHoppyApplication();

            validateIban();
            
            validateNationalAddress();

            validateLowStudyApplication(commandLink);
            
        }
        
        
         function validateIban(){
