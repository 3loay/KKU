        var level, errorNumbers, errorChars, errorSpecialChars, errorMail, errorYear1, errorYear2;
        var langStr, max, degree, errorMaxMark, errorPassword, errorConfirmPassword, errorConfirmEmail, errorNoIdentifications;
        var operation, errorMaxNumber, errorMaxTel , errorUploadImg , errorSocial , errorLang ,errEngCharFound,errArabicCharFound , errorHoppy;
        var isMale;
        var validation = true;
        
        var bridgeValue= "";

	function setErrorMessage(errorNum,errorChar,errorSpChars,errorMails,errorMaxMarks,errorY1,
                                errorY2,lang,levels,errorPasswords,errorConfirmPasswords,
                                errorConfirmEmails, operations, errorMaxNumbers, errorMaxTels , errorUploadImage ,errorSocialAccount,errorLangAdd, errorNoIdents ,errEngFound,errArbCharFound , errorHoppyAdd){
	    errorNumbers = errorNum;
	    errorChars   = errorChar;
	    errorYear1   = errorY1;
	    errorYear2   = errorY2;
	    errorMail    = errorMails;
            langStr      = lang;
            level        = levels;
            errorMaxMark = errorMaxMarks;
	    errorSpecialChars = errorSpChars;
            errorPassword = errorPasswords;
            errorConfirmPassword = errorConfirmPasswords;
            errorConfirmEmail = errorConfirmEmails;
            operation = operations;
            errorMaxNumber = errorMaxNumbers;
            errorMaxTel = errorMaxTels;
            errorUploadImg = errorUploadImage;
            errorSocial = errorSocialAccount;
            errorLang = errorLangAdd;
            errorNoIdentifications = errorNoIdents;
            errEngCharFound = errEngFound;
            errArabicCharFound = errArbCharFound;
            errorHoppy = errorHoppyAdd;
	}

        function popMenu(){
            document.getElementById('hrefImage').onclick();
            document.getElementById('dataApplicationDiv').style.display = "";
        }	
          
	function setDegreeAndMaxChosies(maxs,degrees){
	    max    = maxs;
	    degree = degrees
	}
        
        function showHandicaps(radio){
            var span = document.getElementById("handicapsSpan");
            var healthLabel=document.getElementsByTagName("myForm:health");
            //alert(document.forms['myForm'].elements['myForm:health'][0].value)
            
            if(radio.value == '0'){
               span.style.display ='none'; 
            }
            else{
               span.style.display ='';
            }
        }
        
        
