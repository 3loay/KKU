        function validateGenderApplication(){
            if((document.forms['myForm'].elements['myForm:gender'][0].checked == false) &&
               (document.forms['myForm'].elements['myForm:gender'][1].checked == false)) {
                document.getElementById("myForm:gender").style.backgroundColor='rgb(255,198,132)';
                if(validation)
                    document.getElementById("myForm:gender").focus(); 
                validation = false;
            }
            else{
                document.getElementById("myForm:gender").style.backgroundColor='white';
            }
        }
        
        function validateJobApplication(){
            if(document.forms['myForm'].elements['myForm:job'][0].checked == false) {
               if(document.getElementById("myForm:organization").selectedIndex == 0) {
                  document.getElementById("myForm:organization").style.backgroundColor='rgb(255,198,132)';
                  if(validation)
                     document.getElementById("myForm:organization").focus(); 
                  validation = false;
               }
               else{
                   document.getElementById("myForm:organization").style.backgroundColor='white';
               }
            }
        }


        function validateHoppyApplication(){
            var isBridge  =  document.getElementById('myForm:isBrdge').value;
            var nonSelectedLangs = 0;
            var hoppyId = document.getElementsByName("myForm:hoppyType");
            if(isBridge == false || isBridge == 'false') {
                if (document.forms['myForm'].elements['myForm:hoppy'][1].checked == true) {

                    if(hoppyId.length == 0){
                        return;
                    }

                    var x;
                    for(x=0; x < hoppyId.length; x++){
                        if(hoppyId[x].checked){
                            nonSelectedLangs++;
                        }
                    }

                    if(nonSelectedLangs == 0){
                        alert(errorHoppy);
                        validation = false;
                        hoppyId[0].focus();
                    }
                }
            }
        }
        
