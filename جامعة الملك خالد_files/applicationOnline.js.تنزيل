        function validateHighStudyDegreeCode(){
            var isDiplomPar =  document.getElementById('myForm:isDiplom').value;
            if(isDiplomPar != null){
                if(isDiplomPar == 'true'){
                    return;   
                }
            }
            if(document.getElementById("myForm:degreeCode").value == "-1") {
               document.getElementById("myForm:degreeCode").style.backgroundColor='rgb(255,198,132)';
               validation = false;
               document.getElementById("myForm:degreeCode").focus(); 
            }else{
               document.getElementById("myForm:degreeCode").style.backgroundColor='white';
            }
        }
        
        
        
        function validateNewApplication(){
            if(document.getElementById("myForm:nationalNo").value == "") {
               document.getElementById("myForm:nationalNo").style.backgroundColor='rgb(255,198,132)';
               validation = false;
               document.getElementById("myForm:nationalNo").focus(); 
            }else if(document.getElementById("myForm:nationalNo").value.length != 10){
                document.getElementById("myForm:nationalNo").style.backgroundColor='rgb(255,198,132)';
                validation = false;
                document.getElementById("myForm:nationalNo").focus(); 
            }else{
               document.getElementById("myForm:nationalNo").style.backgroundColor='white';
            }
            
            if (operation == 'false'){
                if(document.getElementById("myForm:password").value == "") {
                   document.getElementById("myForm:password").style.backgroundColor='rgb(255,198,132)';
                   if(validation)
                      document.getElementById("myForm:password").focus(); 
                   validation = false;
                }
                else{
                    document.getElementById("myForm:password").style.backgroundColor='white';
                }
    
                if(document.getElementById("myForm:confirmPassword").value == "") {
                   document.getElementById("myForm:confirmPassword").style.backgroundColor='rgb(255,198,132)';
                   if(validation)
                      document.getElementById("myForm:confirmPassword").focus(); 
                   validation = false;
                }
                else{
                    document.getElementById("myForm:confirmPassword").style.backgroundColor='white';
                }
                
                if(document.getElementById("myForm:email").value == "") {
                   document.getElementById("myForm:email").style.backgroundColor='rgb(255,198,132)';
                   if(validation)
                      document.getElementById("myForm:email").focus(); 
                   validation = false;
                }
                else{
                    document.getElementById("myForm:email").style.backgroundColor='white';
                }
//    
//                if(document.getElementById("myForm:confirmEmail").value == "") {
//                   document.getElementById("myForm:confirmEmail").style.backgroundColor='rgb(255,198,132)';
//                   if(validation)
//                      document.getElementById("myForm:confirmEmail").focus(); 
//                   validation = false;
//                }
//                else{
//                    document.getElementById("myForm:confirmEmail").style.backgroundColor='white';
//                }
                
                var isDiplomPar3 =  document.getElementById('myForm:isDiplom').value;
                if(isDiplomPar3 == 'true' || isDiplomPar3 == true ){ 
                    //---pass
                }else{
                    if(document.getElementById("myForm:appAnswer") != null) { 
                        if(document.getElementById("myForm:appAnswer").value == "") {
                           document.getElementById("myForm:appAnswer").style.backgroundColor='rgb(255,198,132)';
                           if(validation)
                              document.getElementById("myForm:appAnswer").focus(); 
                           validation = false;
                        } else{
                            document.getElementById("myForm:appAnswer").style.backgroundColor='white';
                        }
                    }
                }
                
            }  else{
                if(document.getElementById("myForm:email").value == "") {
                  document.getElementById("myForm:email").style.backgroundColor='rgb(255,198,132)';
                   if(validation)
                      document.getElementById("myForm:email").focus(); 
                   validation = false;
                }
                else{
                    document.getElementById("myForm:email").style.backgroundColor='white';
                }
            }
        }
        
        function validateBasicStudentApplication(){
            if(document.getElementById("myForm:homePhone").value == "") {
               document.getElementById("myForm:homePhone").style.backgroundColor='rgb(255,198,132)';
               if(validation)
                  document.getElementById("myForm:homePhone").focus(); 
               validation = false;
            }
            else{
                document.getElementById("myForm:homePhone").style.backgroundColor='white';
            }
            
            if(document.getElementById("myForm:fnames").value == "") {
               document.getElementById("myForm:fnames").style.backgroundColor='rgb(255,198,132)';
               if(validation)
                  document.getElementById("myForm:fnames").focus(); 
               validation = false;
            } else if (blockEnglishLang(document.getElementById("myForm:fnames"))){
                document.getElementById("myForm:fnames").style.backgroundColor='rgb(255,198,132)';
                alert(errEngCharFound);
                if(validation)
                  document.getElementById("myForm:fnames").focus(); 
                validation = false;
            } else{
                document.getElementById("myForm:fnames").style.backgroundColor='white';
            }
            
            if(document.getElementById("myForm:fatherNames").value == "") {
               document.getElementById("myForm:fatherNames").style.backgroundColor='rgb(255,198,132)';
               if(validation)
                  document.getElementById("myForm:fatherNames").focus(); 
               validation = false; 
            } else if (blockEnglishLang(document.getElementById("myForm:fatherNames"))){
                document.getElementById("myForm:fatherNames").style.backgroundColor='rgb(255,198,132)';
                alert(errEngCharFound);
                if(validation)
                  document.getElementById("myForm:fatherNames").focus(); 
                validation = false;
            } else{
                document.getElementById("myForm:fatherNames").style.backgroundColor='white';
            }
            
            if(document.getElementById("myForm:grandNames").value == "") {
               document.getElementById("myForm:grandNames").style.backgroundColor='rgb(255,198,132)';
               if(validation)
                  document.getElementById("myForm:grandNames").focus(); 
               validation = false;
            } else if (blockEnglishLang(document.getElementById("myForm:grandNames"))){
                document.getElementById("myForm:grandNames").style.backgroundColor='rgb(255,198,132)';
                alert(errEngCharFound);
                if(validation)
                  document.getElementById("myForm:grandNames").focus(); 
                validation = false;
            } else{
                document.getElementById("myForm:grandNames").style.backgroundColor='white';
            }
            
            if(document.getElementById("myForm:familyNames").value == "") {
               document.getElementById("myForm:familyNames").style.backgroundColor='rgb(255,198,132)';
               if(validation)
                  document.getElementById("myForm:familyNames").focus(); 
               validation = false;
            } else if (blockEnglishLang(document.getElementById("myForm:familyNames"))){
                document.getElementById("myForm:familyNames").style.backgroundColor='rgb(255,198,132)';
                alert(errEngCharFound);
                if(validation)
                  document.getElementById("myForm:familyNames").focus(); 
                validation = false;
            } else{
                document.getElementById("myForm:familyNames").style.backgroundColor='white';
            }
            
            if(document.getElementById("myForm:fname").value == "") {
               document.getElementById("myForm:fname").style.backgroundColor='rgb(255,198,132)';
               if(validation)
                  document.getElementById("myForm:fname").focus(); 
               validation = false;
             } else if (blockArabicLang(document.getElementById("myForm:fname"))){
                document.getElementById("myForm:fname").style.backgroundColor='rgb(255,198,132)';
                alert(errArabicCharFound);
                if(validation)
                  document.getElementById("myForm:fname").focus(); 
                validation = false;
            } else{
                document.getElementById("myForm:fname").style.backgroundColor='white';
            }
            
            if(document.getElementById("myForm:fatherName").value == "") {
               document.getElementById("myForm:fatherName").style.backgroundColor='rgb(255,198,132)';
               if(validation)
                  document.getElementById("myForm:fatherName").focus(); 
               validation = false;
             } else if (blockArabicLang(document.getElementById("myForm:fatherName"))){
                document.getElementById("myForm:fatherName").style.backgroundColor='rgb(255,198,132)';
                alert(errArabicCharFound);
                if(validation)
                  document.getElementById("myForm:fatherName").focus(); 
                validation = false;
            } else{
                document.getElementById("myForm:fatherName").style.backgroundColor='white';
            }
            
            if(document.getElementById("myForm:grandName").value == "") {
               document.getElementById("myForm:grandName").style.backgroundColor='rgb(255,198,132)';
               if(validation)
                  document.getElementById("myForm:grandName").focus(); 
               validation = false;
             } else if (blockArabicLang(document.getElementById("myForm:grandName"))){
                document.getElementById("myForm:grandName").style.backgroundColor='rgb(255,198,132)';
                alert(errArabicCharFound);
                if(validation)
                  document.getElementById("myForm:grandName").focus(); 
                validation = false;
            } else{
                document.getElementById("myForm:grandName").style.backgroundColor='white';
            }
            
            if(document.getElementById("myForm:familyName").value == "") {
               document.getElementById("myForm:familyName").style.backgroundColor='rgb(255,198,132)';
               if(validation)
                  document.getElementById("myForm:familyName").focus(); 
               validation = false;
             } else if (blockArabicLang(document.getElementById("myForm:familyName"))){
                document.getElementById("myForm:familyName").style.backgroundColor='rgb(255,198,132)';
                alert(errArabicCharFound);
                if(validation)
                  document.getElementById("myForm:familyName").focus(); 
                validation = false;
            } else{
                document.getElementById("myForm:familyName").style.backgroundColor='white';
            }
            
            var isDiplomPar3 =  document.getElementById('myForm:isDiplom').value;
            if(isDiplomPar3 == 'true' || isDiplomPar3 == true ){ 
                 if(trim(document.getElementById("myForm:mobile").value) == "" || document.getElementById("myForm:mobile").value.length != 12   || document.getElementById("myForm:mobile").value.substring(0,4) != "9665"   ) {
                   document.getElementById("myForm:mobile").style.backgroundColor='rgb(255,198,132)';
                   if(validation)
                      document.getElementById("myForm:mobile").focus(); 
                   validation = false;
                }
                else{
                    document.getElementById("myForm:mobile").style.backgroundColor='white';
                }
            }else{
                if(trim(document.getElementById("myForm:mobile").value) == "" || document.getElementById("myForm:mobile").value.length != 10   || document.getElementById("myForm:mobile").value.substring(0,1) != "0"   ) {
                   document.getElementById("myForm:mobile").style.backgroundColor='rgb(255,198,132)';
                   if(validation)
                      document.getElementById("myForm:mobile").focus(); 
                   validation = false;
                }
                else{
                    document.getElementById("myForm:mobile").style.backgroundColor='white';
                }
            }
                var isDiplomPar =  document.getElementById('myForm:isDiplom').value;
                 if(isDiplomPar == 'true' || isDiplomPar == true ){  
                  if(trim(document.getElementById("myForm:ibanAccount").value) == "" ) {
                       document.getElementById("myForm:ibanAccount").style.backgroundColor='rgb(255,198,132)';
                       if(validation)
                          document.getElementById("myForm:ibanAccount").focus(); 
                       validation = false;
                    } else{
                        document.getElementById("myForm:ibanAccount").style.backgroundColor='white';
                    }
                }
            
            
            
            
            
            if(document.getElementById("myForm:permCity") != null){
                if(document.getElementById("myForm:permCity").value == "-1") {
                    document.getElementById("myForm:permCity").style.backgroundColor='rgb(255,198,132)';
                        if(validation)
                            document.getElementById("myForm:permCity").focus(); 
                    validation = false;
                }
                else{
                    document.getElementById("myForm:permCity").style.backgroundColor='white';
                }
            }

            var radioIndex = 0;
            if(navigator.appName == "Microsoft Internet Explorer"){
                radioIndex = 1;
            }
            
            var jobRadio = document.getElementsByName("myForm:health");
            var i = 0;
            var isChecked = false;
          
            while(jobRadio[radioIndex] != null && i < 2 ){
                if(jobRadio[radioIndex].checked == true){
                    isChecked = true;
                }
                i = i + 1; 
                radioIndex = radioIndex + 1;
                jobRadio = document.getElementsByName("myForm:health");
            }
            
            if(isChecked == false){
                document.getElementById("myForm:health").style.backgroundColor='rgb(255,198,132)';
                if(validation)
                    document.getElementById("myForm:health").focus(); 
                validation = false;
            }else{
                  document.getElementById("myForm:health").style.backgroundColor='white';
            }
            
            var isBridge      =  document.getElementById('myForm:isBrdge').value;
            var isDiplom      =  document.getElementById('myForm:isDiplom').value;
            var isHighStudy   =  document.getElementById('myForm:isHighStudy').value;
            var isImageExiest =  document.getElementById('myForm:isImageExiest').value;

            if((isBridge == 'false' || isBridge == false)){
                 if (isImageExiest == 'false' || isImageExiest == false){
                    alert(errorUploadImg);
                    validation = false;
                }
              }
          }       
        
        
        function validateIsTransferApplication(){
            if(document.forms['myForm'].elements['myForm:isTransfer'][0].checked == true) {
                if(document.getElementById("myForm:transGpa").value == "") {
                   document.getElementById("myForm:transGpa").style.backgroundColor='rgb(255,198,132)';
                   if(validation)
                      document.getElementById("myForm:transGpa").focus(); 
                   validation = false;
                }
                else{
                    document.getElementById("myForm:transGpa").style.backgroundColor='white';
                }
                
                if(document.getElementById("myForm:maxTransGpa").value == "") {
                   document.getElementById("myForm:maxTransGpa").style.backgroundColor='rgb(255,198,132)';
                   if(validation)
                      document.getElementById("myForm:maxTransGpa").focus(); 
                   validation = false;
                }
                else{
                    document.getElementById("myForm:maxTransGpa").style.backgroundColor='white';
                }
            }
        }
        
        function validateBrithDateApplication(){
            if(document.getElementById("myForm:DOBDay").selectedIndex == 0) {
               document.getElementById("myForm:DOBDay").style.backgroundColor='rgb(255,198,132)';
               if(validation)
                  document.getElementById("myForm:DOBDay").focus(); 
               validation = false;
            }
            else{
                document.getElementById("myForm:DOBDay").style.backgroundColor='white';
            }
  
            if(document.getElementById("myForm:DOBMonth").selectedIndex == 0) {
               document.getElementById("myForm:DOBMonth").style.backgroundColor='rgb(255,198,132)';
               if(validation)
                  document.getElementById("myForm:DOBMonth").focus(); 
               validation = false;
            }
            else{
                document.getElementById("myForm:DOBMonth").style.backgroundColor='white';
            }
            
            if(document.getElementById("myForm:DOBYear").selectedIndex == 0) {
               document.getElementById("myForm:DOBYear").style.backgroundColor='rgb(255,198,132)';
               if(validation)
