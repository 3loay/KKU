                   validation = false;
                  } else{
                    document.getElementById("myForm:num3").style.backgroundColor='white';
                }
                
                
                 if(document.getElementById("myForm:num1").value == "") {
                   document.getElementById("myForm:num1").style.backgroundColor='rgb(255,198,132)';
                   if(validation)
                      document.getElementById("myForm:num1").focus(); 
                   validation = false;
                  } else{
                    document.getElementById("myForm:num1").style.backgroundColor='white';
                }
                
                
                
                
                 if(document.getElementById("myForm:numconf3").value == "") {
                   document.getElementById("myForm:numconf3").style.backgroundColor='rgb(255,198,132)';
                   if(validation)
                      document.getElementById("myForm:numconf3").focus(); 
